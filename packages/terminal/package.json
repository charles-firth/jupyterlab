    "@phosphor/coreutils": "^1.3.1",
    "@phosphor/domutils": "^1.1.3",
    "@phosphor/messaging": "^1.3.0",
    "@phosphor/widgets": "^1.9.0",
    "xterm": "~3.13.2"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typedoc": "^0.15.2",
    "typescript": "~3.7.2"
